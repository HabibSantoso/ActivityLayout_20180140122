<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="734dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="75px"
        app:srcCompat="@drawable/subaru" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="162dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/subaru_logo" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="450px"
        android:text="@string/carmodel"
        android:textColor="@color/white"
        android:textSize="48sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnSubmit"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="300px"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="52dp"
        android:layout_height="50dp"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="30px"
        android:outlineProvider="bounds"
        app:srcCompat="@drawable/arrow_left_nav_icon" />
</FrameLayout>